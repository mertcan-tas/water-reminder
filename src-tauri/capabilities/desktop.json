{
  "identifier": "desktop-capability",
  "platforms": ["macOS", "windows", "linux"],
  "windows": ["main"],
  "permissions": [
    "autostart:default",
    "store:default",
    "notification:default",
    "process:default",
    "core:app:allow-default-window-icon",
    "core:window:allow-hide",
    "core:app:allow-app-show",
    "core:window:allow-set-skip-taskbar"
  ]
}
